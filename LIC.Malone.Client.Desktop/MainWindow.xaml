        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Malone" Height="350" Width="600">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="235*"/>
			<ColumnDefinition Width="352*"/>
		</Grid.ColumnDefinitions>

		<Label Content="URL" HorizontalAlignment="Left" Margin="17,43,0,0" VerticalAlignment="Top" Grid.Column="2" Height="26" Width="51"/>
		<TextBox Name="Url" HorizontalAlignment="Left" Height="23" Margin="17,74,-70,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="405" Grid.Column="2"/>
		<Button Content="Send" HorizontalAlignment="Left" Margin="17,97,0,0" VerticalAlignment="Top" Width="75" Grid.Column="2" Height="22" Click="Button_Click"/>
		<ListBox Name="HistoryListBox" VerticalAlignment="Top" HorizontalAlignment="Stretch" SelectionChanged="History_SelectionChanged" SelectionMode="Single"/>



	</Grid>
</Window>
